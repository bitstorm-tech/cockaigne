{
  "plugins": [
    "prettier-plugin-go-template",
    "prettier-plugin-tailwindcss"
  ],
  "trailingComma": "none",
  "printWidth": 120,
  "pluginSearchDirs": false,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": "*.svg",
      "options": {
        "parser": "html"
      }
    }
  ]
}
